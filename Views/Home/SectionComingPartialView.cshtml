<!--=============== SECTION COMING ===============-->
<section class="coming" id="coming">
    <h2 class="heading">Coming Soon</h2>
    <!--Coming Container -->
    <div class="coming-container swiper">
        <div class="swiper-wrapper">
            @foreach(var item in Model.willoutmovs){
                <div class="swiper-slide box">
                    <div class="box-img">
                        <a asp-controller="Home" asp-action="Details" asp-route-id="@item.id"><img src="@item.lilmovimg" alt=""></a>
                    </div>
                    <h3>@item.name</h3>
                    <span>@item.category</span>
                </div>
            }
        </div>

    </div>
</section>